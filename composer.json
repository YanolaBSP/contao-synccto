{
  "name": "yanolateam/contao-synccto",
  "description": "Synchronize multiple contao installations with each other",
  "keywords": [
    "backup",
    "synchronization",
    "management",
    "diff"
  ],
  "type": "contao-bundle",
  "homepage": "http://www.men-at-work.de",
  "license": "LGPL-3.0-or-later",
  "authors": [
      {
          "name": "Yanolateam",
          "email": "baptiste.saintpierre@yanola.fr",
          "homepage": "https://github.com/Yanolateam/",
          "role": "Developer"
      }
  ],
  "support": {
      "issues": "https://github.com/yanolabsp/contao-bcapp-toast/issues",
      "source": "https://github.com/yanolabsp/contao-synccto"
  },
  "require": {
    "php": "^8.0",
    "contao/core-bundle": "^4.4.8",
    "menatwork/contao-multicolumnwizard-bundle": "~3.2",
    "menatwork/ctocommunication": "^4.0.0",
    "contao-community-alliance/dc-general": "^2.1.3"
  },
  "require-dev": {
    "phpcq/all-tasks": "~1.2",
    "contao/manager-bundle": "~4.4"
  },
  "autoload": {
    "psr-4": {
      "MenAtWork\\SyncCto\\": "src"
    },
    "classmap": [
      "classes/"
    ]
  },
  "replace": {
    "contao-legacy/synccto": "*"
  },
  "extra": {
    "contao-manager-plugin": "MenAtWork\\SyncCto\\ContaoManager\\Plugin"
  },
  "version": "0.1"
}
